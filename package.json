{
  "name": "ubahn-app",
  "version": "1.0.0",
  "description": "UBahn search ui api",
  "main": "app.js",
  "scripts": {
    "install-client": "cd client && npm install && npm run build && cd ..",
    "client": "cd client && npm start",
    "start": "node app.js",
    "dev": "concurrently --kill-others-on-fail \"npm start\" \"cd client && npm start\"",
    "create-tables": "node scripts/createTables.js",
    "delete-tables": "node scripts/deleteTables.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "aws-sdk": "^2.627.0",
    "axios": "^0.19.2",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "dynamoose": "^1.8.0",
    "express": "^4.17.1",
    "express-interceptor": "^1.2.0",
    "get-parameter-names": "^0.3.0",
    "http-status-codes": "^1.3.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "multer": "^1.4.2",
    "node-cache": "^5.1.0",
    "swagger-ui-express": "^4.1.4",
    "tc-bus-api-wrapper": "topcoder-platform/tc-bus-api-wrapper.git#feature/auth0-proxy-server",
    "tc-core-library-js": "github:appirio-tech/tc-core-library-js#v2.6.4",
    "uuid": "^7.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "standard": "^14.3.0"
  },
  "engines": {
    "node": "12.x"
  },
  "standard": {
    "ignore": [
      "client"
    ]
  }
}
